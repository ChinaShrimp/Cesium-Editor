{
  "name": "cesiumeditor",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "./node_modules/.bin/electron .",
    "pack": "electron-builder --dir",
    "dist": "npm run dist-win64 && npm run dist-mac",
    "dist-win64": "electron-builder --win --x64",
    "dist-mac": "electron-builder --mac"
  },
  "author": "Lyon Liang",
  "license": "Apache 2.0",
  "dependencies": {
    "cesium": "^1.73.0",
    "codemirror": "^5.58.0",
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}